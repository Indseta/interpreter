// libraries (vector, thread, random, chrono, memory)
use vector; // vector, range


void log_vector(vector<i32> &target) {
    for (const auto &n : target) {
        console.log(n);
    }
}


void main() {
    // Single line comment

    /* Multi line comment
    Hello world
    abcd
    1234 */

    console.clear();

    bool b = true;
    u8 u = 128;
    f32 z = 25 / 4; // always float division

    i32 product, sum;
    product = 1;
    sum = 0;

    for (const auto &i : range(0, 10)) {
        product *= i;
        sum++;
    }

    console.log("Sum: " + sum as string);
    console.log(f"Product: ${product}");

    vector<i32> numbers = {0, 10, 26, 2};
    numbers.push_back(27);
    log_vector(numbers);

    string greet = "world";
    console.log(f"Hello ${greet}");
}